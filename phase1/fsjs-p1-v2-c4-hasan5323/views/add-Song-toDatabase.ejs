<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table border="1" style="text-align: center;">
      <thead>
      <tr>
        <th>No</th>
        <th>title</th>
        <th>bandName</th>
        <th>duration</th>
        <th>genre</th>
        <th>totalVote</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(e=> { %>
      <tr>
        <td><%= e.id %></td>
        <td><%= e.title %></td>
        <td><%= e.bandName %></td>
        <td><%= e.duration %></td>
        <td><%= e.genre %></td>
        <td><%= e.totalVote %></td>
      </tr>
      <% }) %>
    </tbody>
    </table>
  </body>
</html>
